# Environment Configuration Template for SQS Integration

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# SQS Queue URLs
SQS_ROUTE_ASSIGNMENT_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/your-account-id/route-assignment-queue
SQS_INVOICE_GENERATION_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/your-account-id/invoice-generation-queue

# SQS Workers Configuration
ENABLE_SQS_WORKERS=true

# Worker Configuration (Optional)
SQS_WORKER_POLL_INTERVAL=5000
SQS_WORKER_MAX_RETRIES=3
SQS_LONG_POLLING_WAIT_TIME=20
SQS_MAX_MESSAGES_PER_POLL=10
